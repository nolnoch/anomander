    "editor.tokenColorCustomizations": {
        "[Default Dark Modern]": {
            "textMateRules": [
                {
                    "scope": [
                        "comment.block",
                        "punctuation.definition.comment.end",
                        "punctuation.definition.comment.begin"
                    ],
                    "settings": {
                        "foreground": "#425769"
                    }
                },{
                    "scope": [
                        "comment.block.documentation",
                        "comment.block.javadoc.java",
                        "punctuation.definition.comment.begin.documentation",
                        "punctuation.definition.comment.end.documentation"
                    ],
                    "settings": {
                        "foreground": "#2f3693",
                        "fontStyle": "bold"
                    }
                },{
                    "scope": [
                        "storage.type.class.doxygen"
                    ],
                    "settings": {
                        "foreground": "#005461",
                        "fontStyle": "bold"
                    }
                },{
                    "scope":[
                        "comment.line",
                        "punctuation.definition.comment"
                    ],
                    "settings": {
                        "foreground": "#454cac",
                        "fontStyle": "italic"
                    }
                },{
                    "scope": [
                        "variable.other.enummember"
                    ],
                    "settings": {
                        "foreground": "#604eff"
                    }
                },{
                    "scope": [
                        "variable.language",
                        "entity.name.scope-resolution",
                        "keyword.operator.cast"
                    ],
                    "settings": {
                        "foreground": "#21478d"
                    }
                },{
                    "scope": [
                        "keyword.other.using",
                        "entity.name.namespace"
                    ],
                    "settings": {
                        "foreground": "#21478d",
                        "fontStyle": "bold"
                    }
                },{
                    "scope": [
                        "constant.language"
                    ],
                    "settings": {
                        "foreground": "#386eba",
                        "fontStyle": "italic"
                    }
                },{
                    "scope": [
                        "string"
                    ],
                    "settings": {
                        "foreground": "#8b7eff"
                    }
                },{
                    "scope": [
                        "constant.character.escape"
                    ],
                    "settings": {
                        "foreground": "#6055c1"
                    }
                },{
                    "scope": [
                        "storage.type"
                    ],
                    "settings": {
                        "foreground": "#008296"
                    }
                },{
                    "scope": [
                        "keyword.operator.new",
                        "keyword.operator.sizeof"
                    ],
                    "settings": {
                        "foreground": "#008296",
                        "fontStyle": "bold"
                    }
                },{
                    "scope": [
                        "storage.modifier"
                    ],
                    "settings": {
                        "foreground": "#007082",
                        "fontStyle": "italic"
                    }
                },{
                    "scope": [
                        "keyword.control"
                    ],
                    "settings": {
                        "foreground": "#c9c9c9",
                        "fontStyle": "italic"
                    }
                },{
                    "scope": [
                        "keyword.control.directive"
                    ],
                    "settings": {
                        "foreground": "#31a2cc",
                        "fontStyle": "italic"
                    }
                },{
                    "scope": [
                        "constant.numeric"
                    ],
                    "settings": {
                        "foreground": "#4c9fff"
                    }
                },{
                    "scope": [
                        "entity.name.function.preprocessor",
                        "entity.other.attribute"
                    ],
                    "settings": {
                        "foreground": "#994141"
                    }
                },{
                    "scope": [
                        "keyword.operator",
                        "keyword.operator.arithmetic",
                        "keyword.operator.comparison",
                        "keyword.operator.relational",
                        "keyword.operator.logical",
                        "keyword.operator.plus",
                        "keyword.operator.minus",
                        "keyword.operator.multiplication",
                        "keyword.operator.division",
                        "keyword.operator.modulo",
                        "keyword.operator.bitwise",
                        "entity.name.function.operator",
                        "punctuation.terminator",
                        "punctuation.separator"
                    ],
                    "settings": {
                        "foreground": "#7191b7"
                    }
                },{
                    "scope": [
                        "entity.name.function.operator"
                    ],
                    "settings": {
                        "foreground": "#029c8d"
                    }
                },{
                    "scope": [
                        "entity.name.function.call",
                        "entity.name.type",
                        "entity.name.type.parameter"
                    ],
                    "settings": {
                        "foreground": "#477bd9"
                    }
                },{
                    "scope": [
                        "entity.name.function.member",
                        "entity.name.function.call",
                        "entity.name.function.definition"
                    ],
                    "settings": {
                        "foreground": "#00bdaa"
                    }
                },{
                    "scope": [
                        "variable",
                        "variable.other.object.access"
                    ],
                    "settings": {
                        "foreground": "#8ac1ff"
                    }
                },{
                    "scope": [
                        "variable.parameter"
                    ],
                    "settings": {
                        "foreground": "#8ac1ff",
                        "fontStyle": "underline"
                    }
                },{
                    "scope": [
                        "variable.other"
                    ],
                    "settings": {
                        "foreground": "#fbff00"
                    }
                },{
                    "scope": [
                        "variable.other.object.property",
                        "variable.other.property"
                ],
                    "settings": {
                        "foreground": "#5a91cc"
                    }
                },
                {
                    "scope": [
                        "support.type.property-name"
                    ],
                    "settings": {
                        "foreground": "#8ac1ff"
                    }
                },{
                    "scope": [
                        "keyword.other.unit"
                    ],
                    "settings": {
                        "foreground": "#647fa0"
                    }
                }
            ]
        }
    },
    "cmake.configureOnOpen": true,
    "explorer.confirmDelete": false,
    "cmake.additionalCompilerSearchDirs": [
        "C:\\msys64\\ucrt64\\bin\\"
    ],
    "security.workspace.trust.untrustedFiles": "open",
    "workbench.startupEditor": "none",
    "git.confirmSync": false,
    "git.enableSmartCommit": true,
    "explorer.confirmDragAndDrop": false,
    "cmake.showOptionsMovedNotification": false,
    "cmake.pinnedCommands": [
        "workbench.action.tasks.configureTaskRunner",
        "workbench.action.tasks.runTask"
    ],
    "c-cpp-flylint.excludeFromWorkspacePaths": [
        "${workspaceFolder}/build",
        "${workspaceFolder}/configs",
        "${workspaceFolder}/resources"
    ],
    "workbench.iconTheme": "material-icon-theme",
    "git.autofetch": true,
    "gitlens.currentLine.enabled": false,
    "gitlens.codeLens.enabled": false,
    "c-cpp-flylint.ignoreParseErrors": true,
    "c-cpp-flylint.standard": [
        "c20",
        "c++20"
    ],
    "c-cpp-flylint.flexelint.enable": false,
    "c-cpp-flylint.lizard.enable": false,
    "c-cpp-flylint.flawfinder.enable": false,
    "c-cpp-flylint.enable": false,
    "terminal.integrated.fontSize": 16,
    "gitlens.views.commitDetails.files.layout": "tree",
    "doxygen_runner.configuration_file_override": "${workspaceFolder}",
    "codeium.enableConfig": {
        "*": true,
        "glsl": true
    },
    "cmake.parallelJobs": 16,
    "bookmarks.navigateThroughAllFiles": false,
    "bookmarks.sideBar.expanded": true,
    "bookmarks.keepBookmarksOnLineDelete": true,
    "qt-core.qtInstallationRoot": "C:\\Qt",
    "window.zoomLevel": -1.75,
    "githubPullRequests.createOnPublishBranch": "never",
    "cmake.ctest.allowParallelJobs": true,
    "cmake.buildArgs": [
        "--clean-first"
    ],
    "cmake.cmakePath": "C:\\Program Files\\CMake\\bin\\cmake.exe",
    "cmake.allowUnsupportedPresetsVersions": true,
    "editor.semanticHighlighting.enabled": true
}