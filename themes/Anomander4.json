{
    "editor.fontFamily": "InconsolataGO Nerd Font Mono",
    "editor.fontSize": 16,
    "editor.fontWeight": 400,
    "editor.fontLigatures": true,
    "C_Cpp.vcFormat.newLine.beforeOpenBrace.function": "sameLine",
    "workbench.externalBrowser": "edge",
    "editor.bracketPairColorization.enabled": true,
    "editor.guides.bracketPairs" : true,
    "editor.guides.highlightActiveIndentation" : true,
    "editor.guides.bracketPairsHorizontal" : false,
    "editor.semanticTokenColorCustomizations": {
        "[Default Dark Modern]": {
            "enabled": true,
            "rules": {
                "newOperator": {
                    "foreground": "#0090a6",
                    "bold": true
                },
                "macro": {
                    "foreground": "#6f2f2f",
                    "bold": true
                },
                "namespace": {
                    "foreground": "#21478d",
                    "bold": true
                },
                "type": {
                    "foreground": "#3c75df"
                },
                "variable": {
                    "foreground": "#7eb0ea"
                },
                "variable.global": {
                    "foreground": "#994141"
                },
                "parameter": {
                    "foreground": "#7eb0ea",
                    "underline": true
                },
                "property": {
                    "foreground": "#4f7eb0"
                },
                "method": {
                    "foreground": "#3bbaad",
                    "bold": false
                },
                "templateType": {
                    "foreground": "#00bdaa",
                    "bold": false
                },
                "function": {
                    "foreground": "#00bdaa",
                    "bold": false
                },
                "templateFunction": {
                    "foreground": "#00bdaa",
                    "bold": false
                }
            }
        }
    },
